<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<epp xmlns="urn:ietf:params:xml:ns:epp-1.0"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="urn:ietf:params:xml:ns:epp-1.0
     epp-1.0.xsd">
  <command>
    <create>
      <contact:create
       xmlns:contact="urn:ietf:params:xml:ns:contact-1.0"
       xsi:schemaLocation="urn:ietf:params:xml:ns:contact-1.0
       contact-1.0.xsd">
        <contact:id>{{contact-id}}</contact:id>
        <contact:postalInfo type="int">
          <contact:name>{{contact-name}}</contact:name>
          <contact:org>{{contact-org}}</contact:org>
          <contact:addr>
            <contact:street>{{contact-street1}}</contact:street>
            <contact:street>{{contact-street2}}</contact:street>
            <contact:city>{{contact-city}}</contact:city>
            <contact:sp>{{contact-province}}</contact:sp>
            <contact:pc>{{contact-pc}}</contact:pc>
            <contact:cc>{{contact-country}}</contact:cc>
          </contact:addr>
        </contact:postalInfo>
        <contact:voice x="1234">{{contact-phone}}</contact:voice>
        <contact:fax>+1.7035555556</contact:fax>
        <contact:email>jdoe@{{domain-domain}}.{{domain-tld}}</contact:email>
        <contact:authInfo>
          <contact:pw>{{contact-pw}}</contact:pw>
        </contact:authInfo>
        <contact:disclose flag="0">
          <contact:voice/>
          <contact:email/>
        </contact:disclose>
      </contact:create>
    </create>
    <clTRID>ABC-12345</clTRID>
  </command>
</epp>